{
  "name": "mindstrike",
  "version": "0.0.1",
  "description": "An agentic AI knowledge assistant",
  "author": "MindStrike",
  "license": "MIT",
  "type": "module",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "npm run build:server && npm-run-all --parallel dev:server dev:client dev:build-watch",
    "dev:debug": "npm run build:server && npm-run-all --parallel dev:server:debug dev:client dev:build-watch",
    "dev:client": "vite",
    "dev:server": "NODE_ENV=development tsx watch server/index.ts",
    "dev:server:debug": "NODE_ENV=development tsx watch server/index.ts --debug",
    "dev:build-watch": "tsc -p server/tsconfig.json --watch",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit && tsc -p server/tsconfig.json --noEmit",
    "start": "npm run build && node dist/server/server/index.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:tested": "node scripts/lint-tested-files.js",
    "stylelint": "stylelint \"**/*.css\"",
    "stylelint:fix": "stylelint \"**/*.css\" --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run typecheck && npm run lint && npm run stylelint && npm run format:check",
    "generate:openapi": "NODE_ENV=development GENERATE_OPENAPI=true tsx server/generate-openapi.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:server": "vitest run server",
    "test:client": "vitest run --config vitest.config.frontend.ts",
    "test:integration": "vitest run tests/integration/**/*.test.ts",
    "test:e2e": "start-server-and-test dev http://localhost:5173 test:e2e:run",
    "test:e2e:run": "vitest run --config vitest.config.e2e.ts",
    "test:e2e:show": "start-server-and-test dev http://localhost:5173 test:e2e:show:run",
    "test:e2e:show:run": "HEADED=1 vitest run --config vitest.config.e2e.ts ${VITEST_E2E_FILE:-}",
    "mcp:import": "node scripts/import-mcp-config.js",
    "electron": "electron .",
    "electron:dev": "NODE_ENV=development electron .",
    "package": "npm run build && electron-builder",
    "package:dir": "npm run build && electron-builder --dir",
    "package:win": "npm run build && electron-builder --win",
    "package:mac": "npm run build && electron-builder --mac",
    "package:mac:arm64": "npm run build && electron-builder --mac --arm64",
    "package:mac:x64": "npm run build && electron-builder --mac --x64",
    "package:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@fontsource/crimson-pro": "^5.2.6",
    "@fontsource/fira-code": "^5.2.6",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/source-code-pro": "^5.2.6",
    "@fontsource/source-serif-pro": "^5.2.5",
    "@langchain/anthropic": "^0.3.23",
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.60",
    "@langchain/google-genai": "^0.2.13",
    "@langchain/langgraph": "^0.3.4",
    "@langchain/ollama": "^0.2.3",
    "@langchain/openai": "^0.5.14",
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@modelcontextprotocol/server-filesystem": "^2025.3.28",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@monaco-editor/react": "^4.6.0",
    "@types/howler": "^2.2.12",
    "@types/multer": "^2.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-virtualized": "^9.22.2",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dompurify": "^3.2.2",
    "electron-window-state": "^5.0.3",
    "express": "^4.19.2",
    "express-oas-generator": "^1.0.48",
    "fix-path": "^4.0.0",
    "glob": "^11.0.3",
    "howler": "^2.2.4",
    "immer": "^10.1.1",
    "katex": "^0.16.22",
    "langchain": "^0.3.29",
    "lucide-react": "^0.469.0",
    "marked": "^14.1.3",
    "mermaid": "^11.6.0",
    "monaco-editor": "^0.52.0",
    "multer": "^2.0.1",
    "music-metadata": "^11.6.0",
    "node-llama-cpp": "^3.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-katex": "^3.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-virtualized": "^9.22.6",
    "reactflow": "^11.11.4",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "zustand": "^5.0.5"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.24.2",
    "@rollup/rollup-linux-x64-gnu": "^4.28.1",
    "dmg-license": "^1.0.11"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.54.1",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.54.1",
    "postcss": "^8.5.3",
    "prettier": "^3.6.0",
    "start-server-and-test": "^2.0.12",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-prettier": "^5.0.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.4",
    "typescript": "~5.7.2",
    "vite": "^6.0.5",
    "vitest": "^3.2.4"
  },
  "build": {
    "appId": "com.mindstrike.app",
    "productName": "MindStrike",
    "icon": "public/web-app-manifest-512x512.png",
    "directories": {
      "output": "dist/electron"
    },
    "files": [
      "electron/preload.js",
      "electron/main.js",
      "dist/client/**/*",
      "dist/server/**/*",
      "public/favicon.ico",
      "public/web-app-manifest-512x512.png",
      "public/favicon-square.icns",
      "public/favicon-square.iconset/icon_512x512.png",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "public/favicon-square.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        }
      ]
    },
    "dmg": {
      "writeUpdateInfo": false
    },
    "win": {
      "icon": "public/favicon-square.iconset/icon_512x512.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
